        proxy_pass http://skia-proxy:8000;
        proxy_set_header Host "android-master-autoroll-8000-proxy.skia.org";
    }

    location /json/status {
        proxy_pass http://android-master-autoroll:8000/json/status;
        proxy_set_header Host $host;
    }
    location /json/ministatus {
        proxy_pass http://android-master-autoroll:8000/json/ministatus;
        proxy_set_header Host $host;
    }
}
