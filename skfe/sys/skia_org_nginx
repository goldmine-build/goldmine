#####   android-compile.skia.org   ###########################
server {
    listen      443;
    server_name android-compile.skia.org;

    ssl on;

    access_log /var/log/nginx/android-compile.access.log;
    error_log /var/log/nginx/android-compile.error.log error;

    location / {
        proxy_pass http://skia-proxy:8000;
        proxy_set_header Host "android-compile-8002-proxy.skia.org";
    }
    location /_/register {
        proxy_pass http://android-compile:8002/_/register;
        proxy_set_header Host $host;
    }
    location /get_task_status {
        proxy_pass http://android-compile:8002/get_task_status;
        proxy_set_header Host $host;
    }
}
server {
    listen      80;
    server_name android-compile.skia.org;
    return 301 https://android-compile.skia.org$request_uri;
}

#####   android-master-roll.skia.org   ###########################
