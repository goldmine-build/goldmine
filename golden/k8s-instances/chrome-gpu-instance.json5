{
    // Gold settings
    "BIGTABLE_INSTANCE":       "production",
    "BIGTABLE_PROJECT":        "skia-public",
    "BIGTABLE_TABLE":          "gold-chrome-gpu",
    "BUCKET":                  "skia-gold-chrome-gpu",
    "CODE_REVIEW_SYSTEM":      "gerrit",
    "DATASTORE_NAMESPACE":     "gold-chrome-gpu",
    "DATASTORE_PROJECT":       "skia-public",
    "FIRESTORE_NAMESPACE":     "chrome-gpu",
    "FIRESTORE_PROJECT":       "skia-firestore",
    "FORCE_LOGIN":             false,
    "GERRIT_URL":              "https://chromium-review.googlesource.com",
    "GITREPO_URL":             "https://chromium.googlesource.com/chromium/src",
    "N_COMMITS":               512,
    "PUBSUB_PROJECT":          "skia-public",
    "SITE_URL":                "https://chrome-gpu-gold.skia.org",

    // Git BigTable settings
    "GIT_BT_TABLE":            "git-repos2",

    // Settings for skiacorrectness instance
    "AUTHORIZED_PARAMS":       "", // defaults to "all"
    "AUTHORITATIVE":           true,
    "AUTH_USERS":              "google.com",
    "CHANGELIST_TRACKING":     true,
    "CIS_URL_TEMPLATE":        "https://cr-buildbucket.appspot.com/build/%s",
    "CRS_URL_TEMPLATE":        "https://chromium-review.googlesource.com/%s",
    "HASHES_FILE":             "gold-chrome-gpu-hashes.txt",
    "LOGIN_SECRETS":           "skia-org-legacy-login-secrets",
    "MAIN_CPU":                "4",
    "MAIN_MEMORY":             "4Gi",
    "SKCORR_DEFAULT_CORPUS":   "chrome-gpu",
    "SKCORR_DIFF_SERVER":      "gold-chrome-gpu-diffserver",
    "SKCORR_TITLE":            "Chrome GPU Gold",
    "TILE_FRESHNESS":          "1m",

    // Settings for diffserver instance.
    "DIFFSERVER_MEMORY":       "32Gi",
    "DIFFSERVER_CPU":          4,
    "DIFFSERVER_CACHE_SIZE":   10,

    // Settings for the main ingester.
    "ING_RUN_EVERY":           "1h",
    "ING_N_COMMITS":           5,
    "ING_MIN_HOURS":           2,

    // Settings for the Tryjob ingester. If ING_TRY_ENABLED is false no ingester for
    // tryjobs will be configured.
    "ING_TRY_ENABLED":         true,
    "ING_TRY_RUN_EVERY":       "1h",
    "ING_TRY_MIN_HOURS":        2,
    "CODE_REVIEW_SYSTEM":      "gerrit",
    "CI_SYSTEM":               "buildbucket",
}
