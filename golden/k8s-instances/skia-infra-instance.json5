{
  // Gold settings
  "BIGTABLE_INSTANCE":       "production",
  "BIGTABLE_PROJECT":        "skia-public",
  "BIGTABLE_TABLE":          "gold-skia-infra",
  "BUCKET":                  "skia-gold-skia-infra",
  "CODE_REVIEW_SYSTEM":      "gerrit",
  "DATASTORE_NAMESPACE":     "gold-skia-infra",
  "DATASTORE_PROJECT":       "skia-public",
  "FIRESTORE_NAMESPACE":     "skia-infra",
  "FIRESTORE_PROJECT":       "skia-firestore",
  "FORCE_LOGIN":             false,
  "GERRIT_URL":              "https://skia-review.googlesource.com",
  "GITREPO_URL":             "https://skia.googlesource.com/buildbot.git",
  "N_COMMITS":               200,
  "PUBSUB_PROJECT":          "skia-public",
  "SITE_URL":                "https://skia-infra-gold.skia.org",

  // Git BigTable settings
  "GIT_BT_TABLE":            "git-repos2",

  // Settings for skiacorrectness instance
  "AUTH_USERS":              "google.com lovisolo-desktop@skia-public.iam.gserviceaccount.com",
  "AUTHORITATIVE":           true,
  "AUTHORIZED_PARAMS":       "", // defaults to "all"
  "CHANGELIST_TRACKING":     true,
  "CIS_URL_TEMPLATE":        "https://cr-buildbucket.appspot.com/build/%s",
  "CL_COMMENT_DRYRUN":       false,
  "CL_COMMENT_TEMPLATE":     0,
  "CRS_URL_TEMPLATE":        "https://skia-review.googlesource.com/%s",
  "HASHES_FILE":             "gold-skia-infra-hashes.txt",
  "LOGIN_SECRETS":           "skia-org-legacy-login-secrets",
  "MAIN_CPU":                "4",
  "MAIN_MEMORY":             "4Gi",
  "SKCORR_DEFAULT_CORPUS":   "infra",
  "SKCORR_DIFF_SERVER":      "gold-skia-infra-diffserver",
  "SKCORR_TITLE":            "Skia Infra Gold",
  "TILE_FRESHNESS":          "1m",

  // Expectations cleanup policy
  "POSITIVE_EXPECTATIONS_MAX_AGE": "720h", // 30 days
  "NEGATIVE_EXPECTATIONS_MAX_AGE": "4320h", // 180 days

  // Settings for diffserver instance.
  "DIFFSERVER_MEMORY":       "32Gi",
  "DIFFSERVER_CPU":          4,
  "DIFFSERVER_CACHE_SIZE":   10,

  // Settings for the main ingester.
  "ING_RUN_EVERY":           "1h",
  "ING_N_COMMITS":           5,
  "ING_MIN_HOURS":           2,

  // Settings for the Tryjob ingester. If ING_TRY_ENABLED is false no ingester for
  // tryjobs will be configured.
  "ING_TRY_ENABLED":         true,
  "ING_TRY_RUN_EVERY":       "1h",
  "ING_TRY_MIN_HOURS":       2,
  "CODE_REVIEW_SYSTEM":      "gerrit",
  "CI_SYSTEM":               "buildbucket"
}
