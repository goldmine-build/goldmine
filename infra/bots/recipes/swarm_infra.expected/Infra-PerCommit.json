[
  {
    "cmd": [
      "git",
      "init"
    ],
    "cwd": "[START_DIR]/buildbot",
    "name": "git init"
  },
  {
    "cmd": [
      "git",
      "add",
      "."
    ],
    "cwd": "[START_DIR]/buildbot",
    "name": "git add"
  },
  {
    "cmd": [
      "git",
      "commit",
      "-m",
      "Fake commit to satisfy recipe tests"
    ],
    "cwd": "[START_DIR]/buildbot",
    "name": "git commit"
  },
  {
    "cmd": [
      "which",
      "go"
    ],
    "cwd": "[START_DIR]/buildbot",
    "env": {
      "CHROME_HEADLESS": "1",
      "GIT_USER_AGENT": "git/1.9.1",
      "GOCACHE": "[START_DIR]/cache/go_cache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[START_DIR]/cache/gopath",
      "GOROOT": "[START_DIR]/go/go",
      "PATH": "[START_DIR]/go/go/bin:[START_DIR]/cache/gopath/bin:[START_DIR]/gcloud_linux/bin:[START_DIR]/protoc/bin:[START_DIR]/node/node/bin:<PATH>"
    },
    "name": "which go"
  },
  {
    "cmd": [
      "go",
      "mod",
      "download"
    ],
    "cwd": "[START_DIR]/buildbot",
    "env": {
      "CHROME_HEADLESS": "1",
      "GIT_USER_AGENT": "git/1.9.1",
      "GOCACHE": "[START_DIR]/cache/go_cache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[START_DIR]/cache/gopath",
      "GOROOT": "[START_DIR]/go/go",
      "PATH": "[START_DIR]/go/go/bin:[START_DIR]/cache/gopath/bin:[START_DIR]/gcloud_linux/bin:[START_DIR]/protoc/bin:[START_DIR]/node/node/bin:<PATH>"
    },
    "name": "go mod download",
    "~followup_annotations": [
      "@@@STEP_FAILURE@@@"
    ]
  },
  {
    "cmd": [
      "go",
      "mod",
      "download"
    ],
    "cwd": "[START_DIR]/buildbot",
    "env": {
      "CHROME_HEADLESS": "1",
      "GIT_USER_AGENT": "git/1.9.1",
      "GOCACHE": "[START_DIR]/cache/go_cache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[START_DIR]/cache/gopath",
      "GOROOT": "[START_DIR]/go/go",
      "PATH": "[START_DIR]/go/go/bin:[START_DIR]/cache/gopath/bin:[START_DIR]/gcloud_linux/bin:[START_DIR]/protoc/bin:[START_DIR]/node/node/bin:<PATH>"
    },
    "name": "go mod download (2)"
  },
  {
    "cmd": [
      "sudo",
      "npm",
      "i",
      "-g",
      "bower@1.8.2"
    ],
    "cwd": "[START_DIR]/buildbot",
    "env": {
      "CHROME_HEADLESS": "1",
      "GIT_USER_AGENT": "git/1.9.1",
      "GOCACHE": "[START_DIR]/cache/go_cache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[START_DIR]/cache/gopath",
      "GOROOT": "[START_DIR]/go/go",
      "PATH": "[START_DIR]/go/go/bin:[START_DIR]/cache/gopath/bin:[START_DIR]/gcloud_linux/bin:[START_DIR]/protoc/bin:[START_DIR]/node/node/bin:<PATH>"
    },
    "name": "install bower"
  },
  {
    "cmd": [
      "go",
      "run",
      "./run_unittests.go",
      "--alsologtostderr",
      "--small"
    ],
    "cwd": "[START_DIR]/buildbot",
    "env": {
      "CHROME_HEADLESS": "1",
      "GIT_USER_AGENT": "git/1.9.1",
      "GOCACHE": "[START_DIR]/cache/go_cache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[START_DIR]/cache/gopath",
      "GOROOT": "[START_DIR]/go/go",
      "PATH": "[START_DIR]/go/go/bin:[START_DIR]/cache/gopath/bin:[START_DIR]/gcloud_linux/bin:[START_DIR]/protoc/bin:[START_DIR]/node/node/bin:<PATH>:[DEPOT_TOOLS]",
      "SKIABOT_TEST_DEPOT_TOOLS": "[DEPOT_TOOLS]"
    },
    "name": "run_unittests"
  },
  {
    "cmd": [
      "git",
      "diff",
      "--no-ext-diff",
      "--exit-code",
      "go.mod"
    ],
    "cwd": "[START_DIR]/buildbot",
    "name": "git diff go.mod"
  },
  {
    "name": "$result"
  }
]