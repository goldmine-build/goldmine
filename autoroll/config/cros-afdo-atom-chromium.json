{
  "childDisplayName": "ChromeOS Atom AFDO profile",
  "contacts": [
    "tcwang@google.com",
    "denik@google.com"
  ],
  "isInternal": false,
  "ownerPrimary": "borenet",
  "ownerSecondary": "rmistry",
  "parentDisplayName": "Chromium",
  "parentWaterfall": "https://build.chromium.org",
  "rollerName": "cros-afdo-atom-chromium",
  "serviceAccount": "chromium-autoroll@skia-public.google.com.iam.gserviceaccount.com",
  "sheriff": [
    "c-compiler-chrome@google.com"
