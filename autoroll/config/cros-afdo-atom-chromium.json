    "gerrit": {
      "url": "https://chromium-review.googlesource.com",
      "project": "chromium/src",
      "config": "chromium"
    },
    "gcsBucket": "chromeos-prebuilt",
    "gcsPath": "afdo-job/vetted/release",
    "versionFile": "chromeos/profiles/atom.afdo.newest.txt",
    "shortRevRegex": "\\d+-\\d+.\\d+-\\d+-benchmark-\\d+.\\d+.\\d+.\\d+-r\\d+",
    "versionRegex": "^chromeos-chrome-amd64-atom-(\\d+)-\\d+.\\d+-(\\d+)-benchmark-90.(\\d+).(\\d+).(\\d+)-r(\\d+)-redacted.afdo.xz$"
  },
  "kubernetes": {
    "cpu": "1",
    "memory": "2Gi",
    "readinessFailureThreshold": "10",
    "readinessInitialDelaySeconds": "30",
    "readinessPeriodSeconds": "30"
  }
