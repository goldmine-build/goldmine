{
  "childName": "Skia",
  "gerritURL": "https://skia-review.googlesource.com",
  "isInternal": true,
  "parentName": "Internal Test",
  "parentWaterfall": "https://status-internal.skia.org/",
  "rollerName": "skia-internal-autoroll",
  "serviceAccount": "skia-internal-autoroll@skia-corp.google.com.iam.gserviceaccount.com",
  "sheriff": [
    "borenet@google.com"
  ],
  "depsRepoManager": {
    "childBranch": "master",
    "childPath": "skia",
    "parentBranch": "master",
    "preUploadSteps": [
      "TrainInfra"
    ],
    "parentRepo": "https://skia.googlesource.com/internal_test.git",
    "includeBugs": false,
    "includeLog": true
  },
  "cqExtraTrybots": [
    "skia.primary:Housekeeper-PerCommit-InfraTests"
  ],
  "kubernetes": {
    "cpu": "1",
    "memory": "16Gi",
    "disk": "50Gi",
    "readinessInitialDelaySeconds": "600",
    "readinessPeriodSeconds": "60",
    "readinessFailureThreshold": "10"
  },
  "maxRollFrequency": "0m",
  "notifiers": [
    {
      "filter": "warning",
      "email": {
        "emails": [
          "borenet@google.com"
        ]
      }
    }
  ],
  "safetyThrottle": {
    "attemptCount": 0,
    "timeWindow": "1h"
  }
}
