    "url": "https://skia-review.googlesource.com",
    "project": "skia",
    "config": "chromium"
  },
  "copyRepoManager": {
    "childBranch": "master",
    "parentBranch": "master",
    "parentRepo": "https://skia.googlesource.com/skia.git",
    "gerrit": {
      "url": "https://skia-review.googlesource.com",
      "project": "skia",
      "config": "chromium"
