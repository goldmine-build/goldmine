// See https://skia.googlesource.com/buildbot.git/+show/master/autoroll/go/roller/config.go#130
// for documentation of the autoroller config.
{
  "childDisplayName": "Plugins",
  "contacts": [
    "flutter-ecosystem@google.com",
    "rmistry@google.com"
  ],
  "isInternal": false,
  "ownerPrimary": "rmistry",
  "ownerSecondary": "borenet",
  "parentDisplayName": "Flutter",
  "parentWaterfall": "https://flutter-dashboard.appspot.com/#/build",
  "rollerName": "flutter-plugins-flutter-autoroll",
  "serviceAccount": "flutter-autoroll@skia-public.iam.gserviceaccount.com",
  "sheriff": [
    "flutter-ecosystem@google.com"
  ],
  "sheriffBackup": [
    "amirha@google.com"
  ],
  "commitMsg": {
    "childLogURLTmpl": "https://github.com/flutter/plugins/compare/{{.RollingFrom}}...{{.RollingTo}}",
    "includeLog": true,
    "includeRevisionCount": true,
    "template": "default"
  },
  "github": {
    "repoOwner": "flutter",
    "repoName": "flutter",
    "checksWaitFor": [
      "flutter-build"
    ]
  },
  "githubRepoManager": {
    "childBranch": "master",
    "parentBranch": "master",
    "parentRepo": "git@github.com:flutter/flutter.git",
    "childRevLinkTmpl": "https://github.com/flutter/plugins/commit/%s",
    "childRepoName": "plugins",
    "childRepoURL": "git@github.com:flutter/plugins",
    "childUserName": "flutter",
    "forkRepoURL": "git@github.com:engine-flutter-autoroll/flutter.git",
    "revisionFile": "bin/internal/flutter_plugins.version"
  },
  "kubernetes": {
    "cpu": "1",
    "memory": "8Gi",
    "readinessFailureThreshold": "10",
    "readinessInitialDelaySeconds": "600",
    "readinessPeriodSeconds": "60",
    "disk": "50Gi",
    "secrets": [
      {
        "name": "flutter-github-token",
        "mountPath": "/var/secrets/github-token"
      },
      {
        "name": "flutter-ssh-key",
        "mountPath": "/var/secrets/ssh-key"
      }
    ]
  },
  "notifiers": [
    {
      "filter": "warning",
      "email": {
        "emails": [
          "$REVIEWERS"
        ]
      }
    }
  ]
}
